<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
         http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">
         <welcome-file-list>
         <welcome-file> home.jsp </welcome-file>
         </welcome-file-list>
         <servlet>
         <servlet-name>newpage</servlet-name>
         <servlet-class>com.te.servlets.NewPage</servlet-class>
         </servlet>
         <servlet-mapping>
         <servlet-name>newpage</servlet-name>
         <url-pattern>/toNewpage</url-pattern>
         </servlet-mapping>
         
         <servlet>
         <servlet-name>login</servlet-name>
         <servlet-class>com.te.login.Login</servlet-class>
         </servlet>
         <servlet-mapping>
         <servlet-name>login</servlet-name>
         <url-pattern>/welcome</url-pattern>
         </servlet-mapping>
         
		<servlet>
         <servlet-name>add</servlet-name>
         <servlet-class>com.te.add.Result</servlet-class>
         </servlet>
         <servlet-mapping>
         <servlet-name>add</servlet-name>
         <url-pattern>/result</url-pattern>
         </servlet-mapping>
         
         <servlet>
         <servlet-name>add2</servlet-name>
         <servlet-class>com.te.addusingpost.addition</servlet-class>
         </servlet>
         <servlet-mapping>
         <servlet-name>add2</servlet-name>
         <url-pattern>/result2</url-pattern>
         </servlet-mapping>
        
        
         
         
         </web-app>